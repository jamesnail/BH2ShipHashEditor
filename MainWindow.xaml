        <TextBlock HorizontalAlignment="Left" Margin="744,602,0,0" TextWrapping="Wrap" Text="G" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBlock HorizontalAlignment="Left" Margin="829,602,0,0" TextWrapping="Wrap" Text="B" VerticalAlignment="Top" FontWeight="Bold"/>
